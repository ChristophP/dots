{
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "elmLS": {
      "command": "elm-language-server",
      "args": ["--stdio"],
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "diagnosticsOnSaveOnly": true
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "elm",
    "typescript",
    "javascript", "js",
    "json",
    "haskell",
    "css",
    "scss",
    "sass",
    "html",
    "html.handlebars",
    "vue"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "eslint.options": {
    "useEslintrc": true
  },
  "tailwindCSS.cssLanguages": ["css", "less", "postcss", "sass", "scss", "stylus", "vue"],
  "tailwindCSS.jsLanguages": ["javascript", "javascriptreact", "reason", "typescriptreact"],
  "tailwindCSS.htmlLanguages": ["blade", "edge", "eelixir", "ejs", "elixir", "elm", "erb", "eruby", "haml", "handlebars", "html", "HTML (EEx)", "HTML (Eex)", "html.twig", "jade", "leaf", "markdown", "njk", "nunjucks", "php", "razor", "slim", "svelte", "twig", "vue"]
}
